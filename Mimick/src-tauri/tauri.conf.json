{
  "productName": "VoiceApp",
  "version": "0.1.0",
  "identifier": "com.placeholder.app",
  "build": {
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:5173",
    "beforeBuildCommand": "npm run build"
  },
  "app": {
    "windows": [
      {
        "title": "VoiceApp Settings",
        "label": "main",
        "width": 900,
        "height": 640,
        "resizable": true
      }
    ]
  },
  "bundle": {
    "active": true,
    "category": "Productivity",
    "icon": [
      "assets/branding/app-icon.svg"
    ],
    "macOS": {
      "entitlements": "entitlements.plist",
      "exceptionDomain": "",
      "frameworks": [],
      "minimumSystemVersion": "11.0",
      "signingIdentity": null
    }
  },
  "updater": {
    "active": true,
    "endpoints": [
      "https://example.com/voiceapp/latest.json"
    ],
    "dialog": true,
    "pubkey": ""
  }
}